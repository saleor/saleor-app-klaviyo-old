{
  "name": "best-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "prettier --write . && next lint --fix",
    "codegen": "graphql-codegen --config codegen.yml",
    "update-schema": "export $(cat .env.local| xargs) && npx get-graphql-schema $NEXT_PUBLIC_SALEOR_URI > ./src/graphql/schema.graphql; npx zeus ./src/graphql/schema.graphql ./src/graphql/ --apollo"
  },
  "dependencies": {
    "@apollo/client": "^3.5.7",
    "@graphql-codegen/typescript-react-apollo": "^3.2.11",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@prisma/client": "^3.8.0",
    "@saleor/app-bridge": "^0.1.7",
    "@saleor/macaw-ui": "^0.3.3",
    "@types/cors": "^2.8.12",
    "cors": "^2.8.5",
    "graphql": "^15.4.0",
    "next": "12.1.0",
    "node-fetch": "^3.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "2.3.1",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-operations": "2.2.2",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "eslint": "8.5.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "graphql-zeus": "^4.0.4",
    "prettier": "2.5.1",
    "typescript": "4.5.4"
  },
  "license": "MIT"
}
